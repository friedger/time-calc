    "react-autobind": "^1.0.6",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.1.1",
    "react-forms": "jducro/react-forms#fix-react-warnings",
    "react-redux": "^5.0.4",
    "recompose": "^0.27.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.16.0",
    "store": "^2.0.4",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "unidragger": "digitalkaoz/unidragger#patch-1",
    "unipointer": "digitalkaoz/unipointer#patch-1",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "test": "node scripts/test.js --env=jsdom",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch",
    "dev": "concurrently \"npm run client:start\" \"npm run watch-css\"",
    "start": "npm run client:build && npm start --prefix build",
    "critical": "cat ./build/index.html | critical --base=./build --folder=./build --inline --minify > ./build/index_critical.html && mv ./build/index_critical.html ./build/index.html",
    "postinstall": "cd node_modules/react-forms && npm i && make build && cd - && rm -rf node_modules/draggabilly/node_modules && rm -rf node_modules/unidragger/node_modules",
    "client:start": "node scripts/start.js",
    "client:build": "rm -rf build/* && mv .env ._env && npm run build-css && node scripts/build.js && mv ._env .env",
    "client:test": "node scripts/test.js --env=jsdom",
    "client:coverage": "npm run client:test -- --coverage",
    "client:eject": "node scripts/eject.js",
    "server:build": "npm run ssr && npm run sw",
    "deploy": "cd server && npm run deploy",
    "sw": "sw-precache --config=scripts/sw-precache-config.js",
    "cs": "standard --fix src/**/*.js src/**/**/*.js server/src/**/*.js -v --color --parser babel-eslint --env jest --env browser --env node --env serviceWorker",
    "ssr": "react-snapshot && BABEL_ENV=production node_modules/.bin/babel-node scripts/ssr.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
