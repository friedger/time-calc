    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.2.0",
    "critical": "^1.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-redux": "^0.2.0",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "fetch-mock": "^6.0.0",
    "inline-critical": "^4.0.1",
    "jest-enzyme": "^6.0.0",
    "node-sass": "^4.5.2",
    "react-addons-test-utils": "^15.5.1",
    "react-snapshot": "^1.3.0",
    "redux-test-utils": "^0.2.1",
    "standard": "^11.0.0",
    "sw-precache": "^5.0.0",
    "why-did-you-update": "^0.1.0"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "blockstack": "^0.17.2",
    "blockstack-signin-btn": "^1.0.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dialog-polyfill": "^0.4.7",
    "dotenv": "4.0.0",
    "draggabilly": "digitalkaoz/draggabilly#patch-1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "get-size": "digitalkaoz/get-size#patch-1",
    "html-webpack-plugin": "2.29.0",
    "immutable": "^3.8.2",
    "jest": "20.0.4",
    "json2csv": "^4.0.0",
    "material-design-lite": "digitalkaoz/material-design-lite#feature/ssr-capability",
    "md-date-time-picker": "puranjayjain/md-date-time-picker#master",
    "moment": "^2.17.1",
    "moment-duration-format": "^2.2.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.5.8",
    "raf": "3.4.0",
